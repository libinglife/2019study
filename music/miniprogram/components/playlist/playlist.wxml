<!--components/playlist/playlist.wxml-->
<view class="playlist-container" bindtap="goMusicList" data-playlistId="{{playlist.id}}">
	<image class="playlist-img" src="{{playlist.picUrl}}" />
	<text class="playlist-playcount" >
        {{_count}}
    </text>
	<view class="playlist-name">
		{{playlist.name}}
	</view>

</view>

